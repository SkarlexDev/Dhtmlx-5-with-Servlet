<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xmlns:web="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="5.0">

	<servlet>
		<servlet-name>college</servlet-name>
		<servlet-class>com.servlet.CollegeServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>timetable</servlet-name>
		<servlet-class>com.servlet.TimeTableServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>tree</servlet-name>
		<servlet-class>com.servlet.TreeServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.servlet.login.LoginServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>doLogout</servlet-name>
		<servlet-class>com.servlet.login.LogoutServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>formManager</servlet-name>
		<servlet-class>com.servlet.admin.FormManageServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>teacherFormManager</servlet-name>
		<servlet-class>com.servlet.admin.TeacherFormManageServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>com.servlet.admin.AddServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>addTeacher</servlet-name>
		<servlet-class>com.servlet.admin.AddTeacherServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>edit</servlet-name>
		<servlet-class>com.servlet.admin.EditServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>editTeacher</servlet-name>
		<servlet-class>com.servlet.admin.EditTeacherServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>com.servlet.admin.DeleteServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>deleteTeacher</servlet-name>
		<servlet-class>com.servlet.admin.DeleteTeacherServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>newCollege</servlet-name>
		<servlet-class>com.servlet.admin.NewCollegeServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>admin</servlet-name>
		<servlet-class>com.servlet.admin.AdminServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>college</servlet-name>
		<url-pattern>/college</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>timetable</servlet-name>
		<url-pattern>/timetable</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>tree</servlet-name>
		<url-pattern>/tree</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>doLogout</servlet-name>
		<url-pattern>/doLogout</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>formManager</servlet-name>
		<url-pattern>/formManager</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>teacherFormManager</servlet-name>
		<url-pattern>/teacherFormManager</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>addTeacher</servlet-name>
		<url-pattern>/addTeacher</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>edit</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>editTeacher</servlet-name>
		<url-pattern>/editTeacher</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>deleteTeacher</servlet-name>
		<url-pattern>/deleteTeacher</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>admin</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>newCollege</servlet-name>
		<url-pattern>/newCollege</url-pattern>
	</servlet-mapping>


</web-app>